<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:em="http://www.mozilla.org/2004/em-rdf#">
    <Description about="urn:mozilla:install-manifest">
        <em:id>###UUID###</em:id>
        <em:name>Vimperator</em:name>
        <em:version>###VERSION###</em:version>
        <em:type>2</em:type> <!-- extension -->
        <em:unpack>false</em:unpack>
        <em:multiprocessCompatible>false</em:multiprocessCompatible>
        <em:description>Vimperator adds an efficient, Vim-inspired user interface on top of Firefox</em:description>
        <em:creator>Martin Stubenschrott</em:creator>
        <em:homepageURL>http://vimperator.org</em:homepageURL>
        <em:iconURL>chrome://vimperator/skin/icon.png</em:iconURL>
        <em:optionsType>3</em:optionsType>
        <em:optionsURL>liberator://help/options</em:optionsURL>
        <em:updateURL>###UPDATEURL###</em:updateURL>
        <em:targetApplication>
            <Description>
                <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
                <em:minVersion>###MINVERSION###</em:minVersion>
                <em:maxVersion>###MAXVERSION###</em:maxVersion>
            </Description>
        </em:targetApplication>
    </Description>
</RDF>

